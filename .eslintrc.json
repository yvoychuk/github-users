{
    "parserOptions": {
        "ecmaVersion": 8,
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "plugins": ["jest"],
    "env": {
        "jest/globals": true,
        "browser": true
    },
    "rules": {
        "no-console": 1
    }
}